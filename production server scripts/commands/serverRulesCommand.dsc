Server_Rules_Command:
    debug: false
    type: command
    name: rules
    usage: /rules
    description: Display server rules
    script:
        - narrate "<green>Server Rules:"
        - narrate "<aqua>No Stealing"
        - narrate "<aqua>No Griefing (pranks are fine)"
        - narrate "<aqua>No Hacking"
        - narrate "<aqua>Be Nice :D"
        - narrate "<aqua>Use Common Sense"